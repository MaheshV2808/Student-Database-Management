<!DOCTYPE html>
<html>
    <head>
        <title></title>
       <!-- CSS only -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous"></head>
    <body>
        <div class = "container">
            <table class = "table table-bordered table-border text-center shadow">
                <tr>
                    <th>Student Name</th>
                    <th>Roll Number</th>
                    <th>Standard</th>
                    <th>Score</th>
                    <th>Mobile Number</th>
                    <th>Email Address</th>
                    <th>Address</th>
                </tr>
                <% students.forEach(function(student){ %>
                <tr>
                    <td><%= student.Name%></td>
                    <td><%= student.RollNumber%></td>
                    <td><%= student.Standard%></td>
                    <td><%= student.Score%></td>
                    <td><%= student.MobileNo%></td>
                    <td><%= student.EmailAddress%></td>
                    <td><%= student.Address%></td>
                    <td><a href = "/delete/<%= student.RollNumber%>" class = "btn btn-danger btn-sm">Delete</a></td>
                    <td><a href = "/edit/<%= student.RollNumber%>" class = "btn btn-success btn-sm">Edit</a></td>
                </tr>
                <% }) %>
            </table>
        </div>
        
    </body>
</html>